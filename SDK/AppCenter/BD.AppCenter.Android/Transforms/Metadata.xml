<?xml version="1.0" encoding="UTF-8"?>
<metadata>
    
    <!-- Remove types that aren't used in wrapper SDK and as public interface in modules. -->
    <remove-node path="/api/package[@name='com.BD.AppCenter']/class[not(contains(@name,'AppCenter') or @name='DependencyConfiguration')]" />
    <remove-node path="/api/package[@name='com.BD.AppCenter.http']/class[not(@name='HttpResponse' or @name='HttpException')]" />
    <remove-node path="/api/package[@name='com.BD.AppCenter.http']/interface[@name='DefaultHttpClientCallTask.Tracker']" />
    <remove-node path="/api/package[@name='com.BD.AppCenter.channel']/class[not(@name='AbstractChannelListener')]" />
    <remove-node path="/api/package[@name='com.BD.AppCenter.ingestion']/class[not(@name='AbstractAppCenterIngestion')]" />
    <remove-node path="/api/package[@name='com.BD.AppCenter.ingestion.models']/class[@name='CommonProperties' or @name='StartServiceLog']" />
    <remove-node path="/api/package[@name='com.BD.AppCenter.ingestion.models.json']/class[not(@name='AbstractLogFactory')]" />
    <remove-node path="/api/package[@name='com.BD.AppCenter.ingestion.models.one']/class[not(@name='CommonSchemaLog')]" />
    <remove-node path="/api/package[@name='com.BD.AppCenter.ingestion.models.one']/class[@name='CommonSchemaLog']/method" />
    <remove-node path="/api/package[@name='com.BD.AppCenter.ingestion.models.properties']/class[not(@name='TypedProperty')]" />
    <remove-node path="/api/package[@name='com.BD.AppCenter.persistence']" />
    <remove-node path="/api/package[@name='com.BD.AppCenter.utils']/class[not(@name='AppCenterLog' or @name='ApplicationLifecycleListener')]" />
    <remove-node path="/api/package[@name='com.BD.AppCenter.utils']/interface[@name='NetworkStateHelper.Listener']" />
    <remove-node path="/api/package[@name='com.BD.AppCenter.utils.context']" />
    <remove-node path="/api/package[@name='com.BD.AppCenter.utils.storage']" />
    <remove-node path="/api/package[@name='com.BD.AppCenter.utils.crypto']" />
    
    <!-- Fix auto-generated bindings. -->
    <attr path="/api/package[@name='com.BD.AppCenter.ingestion']/class[@name='AbstractAppCenterIngestion']/method[@name='setLogUrl']" name="propertyName"></attr>

    <!-- Explicitly adding bindings for internal methods. -->
    <add-node path="/api/package[@name='com.BD.AppCenter']/class[@name='AppCenter']">
        <method name="unsetInstance" return="void" abstract="false" native="false" synchronized="true" static="true" final="false" deprecated="not deprecated" visibility="public" />
    </add-node>

    <!-- Changing namespaces to C# style. -->
    <attr path="/api/package[@name='com.BD.AppCenter']" name="managedName">BD.AppCenter.Android</attr>
    <attr path="/api/package[@name='com.BD.AppCenter.http']" name="managedName">BD.AppCenter.Android.Http</attr>
    <attr path="/api/package[@name='com.BD.AppCenter.channel']" name="managedName">BD.AppCenter.Android.Channel</attr>
    <attr path="/api/package[@name='com.BD.AppCenter.ingestion']" name="managedName">BD.AppCenter.Android.Ingestion</attr>
    <attr path="/api/package[@name='com.BD.AppCenter.ingestion.models']" name="managedName">BD.AppCenter.Android.Ingestion.Models</attr>
    <attr path="/api/package[@name='com.BD.AppCenter.ingestion.models.json']" name="managedName">BD.AppCenter.Android.Ingestion.Models.Json</attr>
    <attr path="/api/package[@name='com.BD.AppCenter.ingestion.models.one']" name="managedName">BD.AppCenter.Android.Ingestion.Models.One</attr>
    <attr path="/api/package[@name='com.BD.AppCenter.ingestion.models.properties']" name="managedName">BD.AppCenter.Android.Ingestion.Models.Properties</attr>
    <attr path="/api/package[@name='com.BD.AppCenter.utils']" name="managedName">BD.AppCenter.Android.Utils</attr>
    <attr path="/api/package[@name='com.BD.AppCenter.utils.async']" name="managedName">BD.AppCenter.Android.Utils.Async</attr>
</metadata>
